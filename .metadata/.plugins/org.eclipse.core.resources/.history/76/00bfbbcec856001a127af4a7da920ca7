package com.nt.java;

import java.util.HashSet;
import java.util.Set;

public class Test {

	public static void main(String[] args) {
		String s="java";
		char[] ch=s.toCharArray();
		int l=ch.length;
		int count=1;
		Set set=new HashSet();
		for(int i=l-1;i>=0;i--) {
		if(set.add(ch[i])==false) {
			count++;
			
			System.out.println(ch[i]);
			
		}
		}

	}

}
