package com.nt.reverse;

import java.util.Arrays;

public class Test {

	public static void main(String[] args) {
		
		int [] a= {0,1,2,3,4};
		int [] b=new int[a.length];
		int k=0;
		for(int i=a.length-1;i>=0;i--) {
			b[k]=a[i];
			k++;
			//System.out.println(Arrays.toString(b));
			
			
		}

	}

}
