package com.nt.oops;

public class AbstractTest extends B {
	public static void main(String[] args) {
		AbstractTest b=new AbstractTest();
		
		b.m1();
	}


}

 abstract class B {
	 
	 abstract void m2();
	
    static void m1() {
		
		System.out.println("static mtod in abstract");
	}
	
}

  class C{
	  
	 void  m2(){
		  System.out.println("c m2");
	  }
	 
 }